<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:ietf:params:xml:ns:epp-1.0 epp-1.0.xsd">
  <command>
    <update>
      <r:release
        xmlns:r="http://www.nominet.org.uk/epp/xml/std-release-1.0"
        xsi:schemaLocation="http://www.nominet.org.uk/epp/xml/std-release-1.0 
        std-release-1.0.xsd">
        <r:domainName>{domain_name}</r:domainName>
        <r:registrarTag>{domain_registrarTag}</r:registrarTag>
      </r:release>
    </update>
    <clTRID>EPP-{clTRID}</clTRID>
  </command>
</epp>